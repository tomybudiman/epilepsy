    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@store/*": ["./src/redux/*"],
      "@modules/*": ["./src/modules/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "index.d.ts"
  ]
}
